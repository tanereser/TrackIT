<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/960_12_col.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/reset.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/text.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript">
    </script>
    <!--<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>-->
    <script src="http://code.jquery.com/jquery-1.8.2.js"></script>
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.9.0/themes/base/jquery-ui.css" />
    <script src="http://code.jquery.com/ui/1.9.0/jquery-ui.js"></script>
    <script>
        $(function () {
            $("input[type=submit], input[type=button], a.button")
            .button({
                icons: {
                    primary: "ui-icon-plus"
                }
            })
        });
    </script>
</head>
<body>
    <div class="container_12">
        <div class="grid_12">
            <div class="header">
                <div class="header-left">@Html.ActionLink("TrackIT", "Index", "Home", new { }, new { @class = "logo" })<br />
                    A Personal Project Tracking System
                </div>
                <div class="header-right">
                    -
                </div>
            </div>
        </div>
        @RenderBody()
        <div class="grid_12">
            <div class="footer">
                TrackIT by <b>Idenz</b> - 2012
            </div>
        </div>
    </div>
</body>
</html>
